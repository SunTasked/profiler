            "configuration" : {
                "input":"content",
                "encoding":"utf-8",
                "decode_error":"ignore",
                "strip_accents":null,
                "analyzer":"word",
                "preprocessor":null,
                "tokenizer":null,
                "ngram_range":[1, 2],
                "stop_words":null,
                "lowercase":true,
                "token_pattern":"(?u)\\b\\w\\w+\\b",
                "max_df":1.0,
                "min_df":2,
                "max_features":null,
                "vocabulary":null,
                "binary":false
            }
        },
        {
            "extractr_name" : "tfidf-default",
            "extractr_type" : "tfidf",
            "configuration" : {
                "norm":"l2",
                "use_idf":true,
                "smooth_idf":true,
                "sublinear_tf":false
            }
        }
    ]
}